


annotation Table{
    tableName:?String;
}

annotation Column{
    columnName:?String;
}

annotation AutoGen{

}

annotation VarChar{
    length:Integer = 256;
}

case class Test{
    @VarChar(80)
        userName:String;
}

annotation MultiValues{
    name:String;
    length:Integer = 255;
}

@Table("users")
@Doc("Dit is een tabel met users")
case class Users{
    @AutoGen
    id:Long;
    @VarChar(80)
    userName:String;
    @NoToString()
    info:?String;
    @MultiValues("peter",length = 80)
    test:?String;

}